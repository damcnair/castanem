{"_id":"5SEvKs9XDU4uh8n3","name":"Lucius Regains Slot","folder":"6cfU6VNX4Uae4muR","sort":0,"flags":{"core":{"sheetClass":"foundryvtt-simple-calendar.NoteSheet"},"foundryvtt-simple-calendar":{"noteData":{"allDay":true,"calendarId":"default","repeats":0,"order":0,"categories":[],"remindUsers":[],"startDate":{"year":1113,"month":3,"day":13,"hour":0,"minute":0,"seconds":0},"endDate":{"year":1113,"month":3,"day":13,"hour":0,"minute":0,"seconds":0}}}},"pages":[{"name":"Lucius Regains Slot","type":"text","title":{"show":false,"level":1},"text":{"format":1,"content":"<p>Lucius regains his 5th-level spell slot.</p>"},"_id":"QcCQngJBqMoLWq8j","image":{},"video":{"controls":true,"volume":0.5},"src":null,"system":{},"sort":0,"ownership":{"default":-1},"flags":{}}],"ownership":{"B9gnYBRjYSZZpO02":3,"FGPHYlVc2qA4X7ZA":2,"HETKO3yU7ZGsMIw8":2,"X2bPAvcWxrrlGGbv":2,"mSMYndsvygS5avjI":2,"eY0EVyBpVmD4iiZ1":2,"JUPK8PoinF46Nf17":2},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}
