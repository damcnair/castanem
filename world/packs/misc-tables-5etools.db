ing | @creature[Cambion] | Apply the monster's Charisma modifier to its actual AC.","img":"icons/svg/d20-black.svg","weight":1,"range":[28,28],"drawn":false,"_id":"v72flj4nuofj1zip"},{"id":29,"flags":{},"type":0,"text":"Flyby | @creature[Peryton] | —","img":"icons/svg/d20-black.svg","weight":1,"range":[29,29],"drawn":false,"_id":"ojcsezf6jmg99drl"},{"id":30,"flags":{},"type":0,"text":"Frightful Presence | @creature[Ancient black dragon] | Increase the monster's effective hit points by 25% if the monster is meant to face characters of 10th level or lower.","img":"icons/svg/d20-black.svg","weight":1,"range":[30,30],"drawn":false,"_id":"nkflmvncpsrcjmhw"},{"id":31,"flags":{},"type":0,"text":"Grappler | @creature[Mimic] | —","img":"icons/svg/d20-black.svg","weight":1,"range":[31,31],"drawn":false,"_id":"4kjmhs0f7yyul4k2"},{"id":32,"flags":{},"type":0,"text":"Hold Breath | @creature[Lizardfolk] | —","img":"icons/svg/d20-black.svg","weight":1,"range":[32,32],"drawn":false,"_id":"g3zl07f6nlcm56in"},{"id":33,"flags":{},"type":0,"text":"Horrifying Visage | @creature[Banshee] | See Frightful Presence.","img":"icons/svg/d20-black.svg","weight":1,"range":[33,33],"drawn":false,"_id":"cz9rkb09e6owvapo"},{"id":34,"flags":{},"type":0,"text":"Illumination | @creature[Flameskull] | —","img":"icons/svg/d20-black.svg","weight":1,"range":[34,34],"drawn":false,"_id":"c8imw7990jqki7wi"},{"id":35,"flags":{},"type":0,"text":"Illusory Appearance | @creature[Green hag] | —","img":"icons/svg/d20-black.svg","weight":1,"range":[35,35],"drawn":false,"_id":"40jvv7hp1dutm6p5"},{"id":36,"flags":{},"type":0,"text":"Immutable Form | @creature[Iron golem] | —","img":"icons/svg/d20-black.svg","weight":1,"range":[36,36],"drawn":false,"_id":"pb0rkaybdwj0537i"},{"id":37,"flags":{},"type":0,"text":"Incorporeal Movement | @creature[Ghost] | —","img":"icons/svg/d20-black.svg","weight":1,"range":[37,37],"drawn":false,"_id":"pcki89a4uvllw5te"},{"id":38,"flags":{},"type":0,"text":"Innate Spellcasting | @creature