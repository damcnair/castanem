agon turtle]","img":"icons/svg/d20-black.svg","weight":1,"range":[97,99],"drawn":false,"_id":"49mlyay6pvrt96b8"},{"id":11,"flags":{},"type":0,"text":"1 @creature[kraken]","img":"icons/svg/d20-black.svg","weight":1,"range":[100,100],"drawn":false,"_id":"bcb8d5mj6lh8nfve"}],"formula":"d100","replacement":true,"displayRoll":true,"folder":null,"sort":0,"permission":{"default":0,"B9gnYBRjYSZZpO02":3},"flags":{},"_id":"JuiydxG96g1fe3Qq"}
